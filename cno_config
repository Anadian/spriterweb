#ifndef CNO_CONFIG_H
#define CNO_CONFIG_H

#ifdef __cplusplus
extern "C" {
#endif //__cplusplus

#include <cno_build.h>

typedef struct CNO_LowLevelConfig_struct {
	cno_u8 audio;
	cno_u8 threads;
	cno_u8 windows;
	cno_u8 debug;
} CNO_LowLevelConfig_type;

cno_u8 CNO_LowLevelConfig_Getaudio();
cno_u8 CNO_LowLevelConfig_Setaudio(cno_u8 audio);
cno_u8 CNO_LowLevelConfig_Getthreads();
cno_u8 CNO_LowLevelConfig_Setthreads(cno_u8 threads);
cno_u8 CNO_LowLevelConfig_Getwindows();
cno_u8 CNO_LowLevelConfig_Setwindows(cno_u8 windows);
cno_u8 CNO_LowLevelConfig_Getdebug();
cno_u8 CNO_LowLevelConfig_Setdebug(cno_u8 debug);

#ifdef __cplusplus
}
#endif //__cplusplus

#endif //CNO_CONFIG_H
